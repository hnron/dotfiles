
let g:python_path  = expand("$HOME/.pyenv/versions/neovim2/bin/")
let g:python3_path = expand("$HOME/.pyenv/versions/neovim3/bin/")
let g:ruby_path    = expand("$HOME/.rbenv/versions/2.6.3/bin/")
let g:node_path    = expand("$HOME/.nodenv/versions/nvim/bin/")

let g:python_host_prog  = g:python_path . "python"
let g:python3_host_prog = g:python3_path . "python"
let g:ruby_host_prog    = g:ruby_path . "neovim-ruby-host"
let g:node_host_prog    = g:node_path . "neovim-node-host"

